{
  "name": "@open-socket/testing-utils",
  "version": "1.0.0",
  "description": "Testing utilities for OpenSocket",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage .turbo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-socket/opensocket-js.git",
    "directory": "packages/testing-utils"
  },
  "keywords": [
    "opensocket",
    "testing",
    "mock",
    "test-utils",
    "real-time"
  ],
  "author": "OpenSocket Contributors",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/open-socket/opensocket-js/issues"
  },
  "homepage": "https://opensocket.dev",
  "dependencies": {
    "@open-socket/core": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@open-socket/core": "workspace:*"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false
}
